@{
    ViewData["Title"] = "About";
    var pr = ViewBag.prt;

}

<h1>GetAllData</h1>

<table class="table table-bordered">
    <tr>
        <th>Product Id</th>
        <th>Product Name</th>
        <th>Product Description</th>
        <th>Product Price</th>
    </tr>

        @foreach (var item in pr)
        {
<tr>
    <td>@item.Id</td>
    <td>@item.Name</td>
    <td>@item.Description</td>
    <td>@item.Price</td>
    <td><a href ="#">Edit</a></td>
</tr>

        }


    
</table>
